{
  "name": "example8",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "cd ../.. && yarn && cd - && yarn && yarn db:create && yarn codegen && yarn db:seed:dev",
    "build": "./tools/scripts/build.sh",
    "codegen": "yarn warthog codegen",
    "compile": "rm -rf ./dist && yarn tsc",
    "config": "dotenvi -s ${WARTHOG_ENV:-local}",
    "db:create": "WARTHOG_DB_OVERRIDE=true yarn warthog db:create",
    "db:drop": "WARTHOG_DB_OVERRIDE=true yarn warthog db:drop",
    "db:seed:dev": "WARTHOG_DB_OVERRIDE=true ts-node tools/seed.ts",
    "start": "WARTHOG_ENV=production yarn run config && node ./dist/examples/09-production/src/index.js",
    "//": "Allows us to use the local warthog CLI in commands above",
    "warthog": "../../bin/warthog"
  },
  "devDependencies": {
    "@types/faker": "^4.1.7",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.5",
    "dotenvi": "^0.9.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.7.2"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.test\\.ts$"
    ]
  }
}
