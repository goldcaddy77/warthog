{
  "name": "example6",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "cd ../.. && yarn && cd - && yarn && yarn db:create && yarn codegen && yarn db:seed:dev",
    "codegen": "warthog codegen",
    "db:create": "warthog db:create",
    "db:drop": "warthog db:drop",
    "db:seed:dev": "ts-node tools/seed.ts",
    "playground": "warthog playground",
    "start": "yarn start:ts",
    "start:debug": "yarn start:ts --inspect",
    "start:ts": "ts-node --type-check src/index.ts",
    "test": "jest --detectOpenHandles --verbose --coverage",
    "test:watch": "jest --watch",
    "//": "Allows us to use the local warthog CLI in commands above",
    "warthog": "../../bin/warthog",
    "watch:ts": "nodemon -e ts,graphql -x ts-node --type-check src/index.ts"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "reflect-metadata": "^0.1.12",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.test\\.ts$"
    ]
  }
}
