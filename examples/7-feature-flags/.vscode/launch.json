{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ts-node-7",
      "type": "node",
      "request": "launch",
      "args": ["${relativeFile}"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
			"env": {
        "TS_NODE_IGNORE": "false",
        "DEBUG": "*"
			}
    }
  ]
}
